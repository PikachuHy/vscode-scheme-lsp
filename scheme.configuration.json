{
  "comments": {
    "lineComment": ";"
  },
  "brackets": [
    ["(", ")"],
    ["[", "]"],
    ["{", "}"]
  ],
  "autoClosingPairs": [
    {"open": "(", "close": ")"},
    {"open": "[", "close": "]"},
    {"open": "{", "close": "}"}
  ],
  "surroundingPairs": [
    {"open": "(", "close": ")"},
    {"open": "[", "close": "]"},
    {"open": "{", "close": "}"}
  ],
  "wordPattern": "[\\w!$%&*+,./:<=>?@^_`|~^-]+",
  "folding": {
    "markers": {
      "start": "\\(\\|{",
      "end": "\\)\\|}"
    }
  },
  "indentationRules": {
    "increaseIndentPattern": "^\\s*(define|lambda|let|let\\*|letrec|letrec\\*|begin|if|cond|case|do|delay|set!|quasiquote|unquote-splicing)\\b",
    "decreaseIndentPattern": "^\\s*\\)"
  },
  "symbols": {
    "boolean": ["#t", "#f"],
    "bracket": ["(", ")", "[", "]", "{", "}"],
    "identifier": ["define", "lambda", "let", "let*", "letrec", "letrec*", "begin", "if", "cond", "case", "do", "delay", "set!", "quasiquote", "unquote-splicing"],
    "number": "0123456789",
    "operator": ["+", "-", "*", "/", "=", ">", "<", ">=", "<=", "eq?", "eqv?", "equal?", "not", "and", "or"],
    "string": "\""
  }
}
